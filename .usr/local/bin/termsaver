#!/usr/bin/env python3


import os
import time
from random import choice


def main(argv):
	cmds = {
		"asciiquarium": "asciiquarium",
		"cmatrix": "cmatrix -s",
		"pipes.sh": "pipes.sh"
	}
	ss = []
	for key in cmds:
		if os.system("hash %s 2>/dev/null" % key) == 0:
			ss.append(cmds[key])
	os.system(choice(ss))


if __name__=="__main__":
	import sys
	if len(sys.argv) > 1:
		main(sys.argv[1:])
	else:
		main([])
